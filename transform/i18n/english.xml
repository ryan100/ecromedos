<language name="english" id="en">

	<settings format="latex">
		<babel territory="US"><select>american</select></babel>
		<babel territiry="AU"><select>australian</select></babel>
		<babel territory="GB"><select>british</select></babel>
		<babel territory="CA"><select>canadian</select></babel>
		<babel territory="NZ"><select>newzealand</select></babel>
	</settings>

	<dictionary>
