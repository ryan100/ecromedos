		<translation key="part">Part %n.</translation>
		<translation key="abstract">Abstract</translation>
		<translation key="index">Index</translation>
		<translation key="glossary">Glossary</translation>

		<translation key="nav-prev">Prev</translation>
		<translation key="nav-next">Next</translation>
		<translation key="nav-up">Up</translation>

		<translation key="sectionnumber">%n.</translation>

